<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="CS5200DBMS">
	<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
	<class>projectDAO.Vehicle</class>
	<class>projectDAO.Request</class>
	<class>projectDAO.VehicleShiftMapping</class>
	<class>projectDAO.Mbtaroute</class>
	<class>projectDAO.User</class>
	<class>projectDAO.ShiftTime</class>

	
	<properties>
			<property name="eclipselink.jdbc.password" value="password"/>
			<property name="eclipselink.jdbc.user" value="root"/>
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost/cs5200db"/>
			<!-- property name="eclipselink.ddl-generation" value="drop-and-create-tables"/-->
			<property name="eclipselink.logging.level" value="ALL"/>
		</properties>
	</persistence-unit>
</persistence>

